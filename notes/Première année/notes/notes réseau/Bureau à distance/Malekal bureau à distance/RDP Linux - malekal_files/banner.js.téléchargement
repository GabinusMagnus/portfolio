var ad=0;function init(){checkAdsBlocked(function(e){e?(console.log("Adblock detecte -- "+ad),ad=2):console.log("Pas Adblock -- "+ad),"2"!=ad&&"1"!=ad||(console.log("On affiche le message car Adblock -- "+ad),document.getElementById("Kev").style.display="block")})}"function"==typeof window.__tcfapi?(window.__tcfapi("getTCData",2,function(e,n){e.vendor.consents[755]||(ad=1,console.log("Consent refusé (1) -- "+ad),document.getElementById("Kev").style.display="block"),e.vendor.consents[755]&&(console.log("Content accepté (1) -- "+ad),2!=ad&&(ad=0))}),window.__tcfapi("addEventListener",2,function(e,n){n&&(e.vendor.consents[755]&&(2!=ad&&(ad=0),console.log("Consent accepté (2) -- "+ad)),0==ad&&(document.getElementById("Kev").style.display="none",console.log("On retire le message car Consent OK sans Adblock")))})):document.getElementById("Kev").style.display="block",document.addEventListener("DOMContentLoaded",init,!1);var ADS_URL="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js";function checkAdsBlocked(e){var n=new XMLHttpRequest;n.onreadystatechange=function(){n.readyState==XMLHttpRequest.DONE&&e(0===n.status||n.responseURL!==ADS_URL)},n.open("HEAD",ADS_URL,!0),n.send(null)}